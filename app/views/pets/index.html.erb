<% provide(:title, 'All pets') %>
<h1>All pets</h1>

<%= will_paginate %>
<ul class="pets">
  <% @pets.each do |pet| %>
  
    <li>
      <%= link_to pet.nickname, pet %>
      <%= pet.age %>
      <%= (pet.gender==0?"Male":"Female") %>
      <%= pet.breeds %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
