<% provide(:title, @pet.nickname) %>
<h1>
<%= @pet.nickname %>
</h1>
<h2>
	<%= "Age:" %>

	<%= @pet.age %>
	
</h2>
<h2>
	<%= "Gender:" %>
	<%= @pet.gender==0?"Male":"Female" %>
</h2>
<h2>
	<%= "Breeds:" %>
	<%= @pet.breeds %>
</h2>
<h2>
	<%= "City:" %>
	<%= @pet.city %>
</h2>
<h2>
	<%= "Owner's email:" %>
	<%= @pet.owner_email %>
</h2>

  



