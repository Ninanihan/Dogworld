<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>
<table class="table table-striped">
   <tbody>


<ul class="users">
  <% @users.each do |user| %>
   <td>
    
    <li>
      <%= gravatar_for user, size: 50 %>
      <br/>
      <%= link_to user.name, user %>
    </li>
  </td>
  <% end %>
</ul>

</table>

<%= will_paginate %>

